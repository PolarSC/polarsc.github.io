<div class="container-fluid main-post-listing-container">
    <div class="row">
      <% site.posts.sort('date',-1).each(function (post) { %>
        <%- partial('_partial/list-view', { post: post}) %>
      <% }) %>
    </div>
  </div>
  <% if (page.total > 1){ %>
    <section id="nav-wrapper">
      <nav id="page-nav">
        <%- paginator({
          prev_text: "« prev",
          next_text: "next »"
        }) %>
      </nav>
    </section>
  <% } %>